class Box{
			double width;
			double height;
			double depth;
	Box(){													//constructer for box
			System.out.println("constructing box");
			width=10;
			height=10;
			depth=30;
		}
		
		double volume(){
			return width*height*depth;
		}
}
	
class Boxdemo5{
		public static void main(String[]args){
					Box mybox1=new Box();
					Box mybox2=new Box();
					double vol;							//vol is the variable which will store the volume
					vol=mybox1.volume();				//for box 1 and box 2 are declared
					System.out.println("volume for box "+vol);
					vol=mybox2.volume();
					System.out.println("volume of box "+vol);
				}
}
